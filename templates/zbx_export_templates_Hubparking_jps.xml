            <discovery_rules>
                <discovery_rule>
                    <name>JPS Devices Discovery</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>system.run[c:\cygwin64\bin\bash.exe -l -c &quot;export PGPASSWORD=jbl; echo  {$JPSDISCOVER1}{$JPSDISCOVER2}{$JPSDISCOVER3}| base64 -d | perl&quot;]</key>
                    <delay>1h</delay>
                    <lifetime>7d</lifetime>
                    <description>The Base64 string is a Perl script to query the JBL db and export a Json with the JPS devices. See the macros.</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>JPS ({#JPSUID}) Version</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>system.run[c:\cygwin64\bin\bash.exe -l -c &quot;curl -s -m 5 {#JPSIP}:65000/jps/api/command/version &quot;]</key>
                            <delay>1h</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>JPS Devices Info</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.version</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>JPS Device ({#JPSUID}) Information</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>system.run[c:\cygwin64\bin\bash.exe -l -c &quot;export JPS={#JPSUID}; export PGPASSWORD=jbl; echo {$JPSINV1}{$JPSINV2}{$JPSINV3}{$JPSINV4} |base64 -d |perl&quot;]</key>
                            <delay>15s</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>JPS Devices Info</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[*]</params>
                                </step>
                                <step>
                                    <type>REGEX</type>
                                    <params>(\[&quot;)(.*)(&quot;,&quot;)(.*)(&quot;,&quot;)(.*)(&quot;.&quot;)(.*)(&quot;\])
UID: \2 IP: \4 STATUS: \6 TYPE: \8</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{iregexp(Offline)}&gt;0</expression>
                                    <name>JPS ({#JPSUID}) with IP ({#JPSIP}) Type ({#JPSTYPE}) is Offline</name>
                                    <priority>HIGH</priority>
                                    <dependencies>
                                        <dependency>
                                            <name>Zabbix agent is not available (or nodata for {$AGENT.NODATA_TIMEOUT})</name>
                                            <expression>{Template Module Zabbix agent active:agent.ping.nodata({$AGENT.NODATA_TIMEOUT})}=1</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
